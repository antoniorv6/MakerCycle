{
  "name": "makercycle-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:mobile": "next build && npx cap sync",
    "start": "next start",
    "lint": "eslint src/ app/",
    "lint:fix": "eslint src/ app/ --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "validate": "pnpm run lint && pnpm run type-check",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:diff": "supabase db diff",
    "db:status": "supabase status",
    "cap:sync": "npx cap sync",
    "cap:build": "pnpm run build && npx cap sync",
    "cap:run:android": "pnpm run cap:build && npx cap run android",
    "cap:run:ios": "pnpm run cap:build && npx cap run ios",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "emulator:android": "pnpm build && npx cap sync android && npx cap run android",
    "emulator:ios": "pnpm build && npx cap sync ios && npx cap run ios",
    "dev:android": "npx cap run android --livereload --external",
    "dev:ios": "npx cap run ios --livereload --external",
    "docker:build": "docker build -t makercycle:latest .",
    "docker:run": "docker run -p 8080:80 makercycle:latest"
  },
  "dependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.1.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@supabase/ssr": "0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "jszip": "^3.10.1",
    "lucide-react": "^0.468.0",
    "next": "16.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.7.0",
    "skills": "^1.3.8",
    "three": "^0.182.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.1.0",
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.1.6",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^20.19.30",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.182.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
